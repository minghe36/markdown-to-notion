    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "showdown": "^2.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.8.0",
    "dotenv-cli": "^8.0.0",
